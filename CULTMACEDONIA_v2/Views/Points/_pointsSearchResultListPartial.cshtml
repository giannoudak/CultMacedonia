@model List<CULTMACEDONIA_v2.Models.PointSearchListViewModel>

 

@{
    int groupSize = 6;
    int total = Model.Count;
    int groups = total / groupSize;

}

<div class="alert alert-info" role="alert">@total @CultResources.Shared.Points found!</div>

<!--  Outer wrapper for presentation only, this can be anything you like -->
<div id="search-slide">

    <!-- start Basic Jquery Slider -->
    <ul class="bjqs">

        @*για καθε group κανω και ενα li*@

        @for (int i = 0; i<= groups; i++)
        {

            <li >
                @if (i == 0)
                {
                <div class="item active" >
                    @foreach (var item in Model.Skip(i * groupSize).Take(groupSize))
                    {

                        <a href="#" class="itmpoint list-group-item" data-id="@item.PointId" data-category="@item.PointCategory" data-x="@item.PointX" data-y="@item.PointY">


                            <div class="row">

                                <div class="itm-img-wrap pull-left">

                                    @if (item.pointSingleImage != null)
                                    {
                                        <img alt="@item.PointName" class="media-object" style="width: 64px; height: 64px;"
                                             src="@item.pointSingleImage.ImageFilePath">
                                    }
                                    else
                                    {
                                        <img class="media-object" alt="CultMacedonia" title="CultMacedonia"
                                             src="data:image/png;base64,"
                                             style="width: 64px; height: 64px;" />
                                    }
                                </div>

                                <div class="col-lg-9">

                                    <h5 class="list-group-item-heading">@item.PointName</h5>
                                    <p class="list-group-item-text">
                                        @item.PointAddress
                                    </p>
                                </div>

                            </div>

                        </a>


                    }
                </div>
                }
                else
                {
                <div class="item">
                    @foreach (var item in Model.Skip(i * groupSize).Take(groupSize))
                    {

                        
                        
                        <a href="#" class="itmpoint list-group-item" data-id="@item.PointId" data-category="@item.PointCategory" data-x="@item.PointX" data-y="@item.PointY">


                            <div class="row">

                                <div class="itm-img-wrap pull-left" >

                                    @if (item.pointSingleImage != null)
                                    {
                                        <img alt="@item.PointName" class="media-object" style="width: 64px; height: 64px;"
                                             src="@item.pointSingleImage.ImageFilePath">
                                    }
                                    else
                                    {
                                        <img class="media-object" alt="CultMacedonia" title="CultMacedonia"
                                             src="data:image/png;base64,"
                                             style="width: 64px; height: 64px;" />
                                    }
                                </div>

                                <div class="col-lg-9">

                                    <h5 class="list-group-item-heading">@item.PointName</h5>
                                    <p class="list-group-item-text">
                                        @item.PointAddress
                                    </p>
                                </div>

                            </div>

                            
                        </a>

                    }
                </div>
                }

            </li>
            
        }


        

        
    </ul>
    <!-- end Basic jQuery Slider -->

</div>
<!-- End outer wrapper -->




<script type="text/javascript">

    $('#search-slide').bjqs({
        animtype: 'slide',
        height: 480,
        width: 325,
        automatic:false,
        responsive: true,
        randomstart: true,
        keyboardnav: true,  // enable keyboard navigation
        showmarkers: false,  // show individual slide markers
        hoverpause: true    // pause the slider on hover
    });



    

</script>
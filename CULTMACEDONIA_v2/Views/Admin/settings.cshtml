@{
    ViewBag.Title = "settings";
    Layout = "~/Views/Shared/_MasterAdmin.cshtml";
}




<div class="content-header">
    <h1>
        <a id="menu-toggle" href="#" class="btn btn-default"><i class="icon-reorder"></i></a>
        Simple Sidebar
    </h1>


</div>
<!-- Keep all page content within the page-content inset div! -->
<div class="page-content inset">

    <ol class="breadcrumb">
        <li><a href="index.html"><i class="icon-dashboard"></i> Dashboard</a></li>
        <li class="active"><i class="icon-file-alt"></i> Blank Page</li>
    </ol>


    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-success ">
                Welcome to SB Admin by <a href="http://startbootstrap.com" class="alert-link">Start Bootstrap</a>! Feel free to use this template for your admin needs! We are using a few different plugins to handle the dynamic tables and charts, so make sure you check out the necessary documentation links provided.
            </div>
            <p class="lead">This simple sidebar template has a hint of JavaScript to make the template responsive. It also includes Font Awesome icon fonts.</p>
        </div>

    </div>

    <div class="row">
        <div class="col-lg-3">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            <i class="fa fa-comments fa-5x"></i>
                        </div>
                        <div class="col-xs-6 text-right">
                            <p class="announcement-heading">456</p>
                            <p class="announcement-text">New Mentions!</p>
                        </div>
                    </div>
                </div>
                <a href="#">
                    <div class="panel-footer announcement-bottom">
                        <div class="row">
                            <div class="col-xs-6">
                                View Mentions
                            </div>
                            <div class="col-xs-6 text-right">
                                <i class="fa fa-arrow-circle-right"></i>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-warning">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            <i class="fa fa-check fa-5x"></i>
                        </div>
                        <div class="col-xs-6 text-right">
                            <p class="announcement-heading">12</p>
                            <p class="announcement-text">To-Do Items</p>
                        </div>
                    </div>
                </div>
                <a href="#">
                    <div class="panel-footer announcement-bottom">
                        <div class="row">
                            <div class="col-xs-6">
                                Complete Tasks
                            </div>
                            <div class="col-xs-6 text-right">
                                <i class="fa fa-arrow-circle-right"></i>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            <i class="fa fa-tasks fa-5x"></i>
                        </div>
                        <div class="col-xs-6 text-right">
                            <p class="announcement-heading">18</p>
                            <p class="announcement-text">Crawl Errors</p>
                        </div>
                    </div>
                </div>
                <a href="#">
                    <div class="panel-footer announcement-bottom">
                        <div class="row">
                            <div class="col-xs-6">
                                Fix Issues
                            </div>
                            <div class="col-xs-6 text-right">
                                <i class="fa fa-arrow-circle-right"></i>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="panel panel-success">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            <i class="fa fa-comments fa-5x"></i>
                        </div>
                        <div class="col-xs-6 text-right">
                            <p class="announcement-heading">56</p>
                            <p class="announcement-text">New Orders!</p>
                        </div>
                    </div>
                </div>
                <a href="#">
                    <div class="panel-footer announcement-bottom">
                        <div class="row">
                            <div class="col-xs-6">
                                Complete Orders
                            </div>
                            <div class="col-xs-6 text-right">
                                <i class="fa fa-arrow-circle-right"></i>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-4">

            <table id="categories" class="table" data-bind="visible: categories().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Category Name</th>
                        <th><button data-bind='click: addCategory'>Add a contact</button></th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: categories">
                    <tr>
                        <td data-bind="text: CategoryId"></td>
                        <td data-bind="text: CategoryName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
        <div class="col-lg-4">
            <table id="properties" class="table" data-bind="visible: properties().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Property Name</th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: properties">
                    <tr>
                        <td data-bind="text: PropertyId"></td>
                        <td data-bind="text: PropertyName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
        <div class="col-lg-4">
            <table id="eras" class="table" data-bind="visible: eras().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Property Name</th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: eras">
                    <tr>
                        <td data-bind="text: EraId"></td>
                        <td data-bind="text: EraName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-4">
            <table id="protectionlevels" class="table" data-bind="visible: protectionlevels().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Protection Level Name</th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: protectionlevels">
                    <tr>
                        <td data-bind="text: ProtectionId"></td>
                        <td data-bind="text: ProtectionName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
        <div class="col-lg-4">
            <table id="ethnologicals" class="table" data-bind="visible: ethnologicals().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Ethnological Name</th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: ethnologicals">
                    <tr>
                        <td data-bind="text: EthnologicalId"></td>
                        <td data-bind="text: EthnologicalName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
        <div class="col-lg-4">
            <table id="religions" class="table" data-bind="visible: religions().length > 0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Religion Name</th>
                    </tr>
                </thead>
                <tbody data-bind="foreach: religions">
                    <tr>
                        <td data-bind="text: ReligionId"></td>
                        <td data-bind="text: ReligionName"></td>

                        <td>
                            <button data-bind="click: $root.edit">Edit</button>
                            <button data-bind="click: $root.delete">Delete</button>

                        </td>

                    </tr>
                </tbody>
            </table>
        </div>
    </div>


</div>


@section Scripts{
    <script src="~/Scripts/knockout-3.0.0.debug.js"></script>
    <script src="~/Scripts/knockout.mapping-latest.debug.js"></script>
    <script src="~/Scripts/ko-sysparams.js"></script>

    
}
